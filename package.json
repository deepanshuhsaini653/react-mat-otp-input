{
  "name": "react-mat-otp-input",
  "version": "1.0.3",
  "description": "This is a OTP input library",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "private": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "React OTP",
    "OTP Library",
    "OTP"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "npx rimraf dist && prettier --write src/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
  },
  "author": "Deepak Saini",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deepanshuhsaini653/react-mat-otp-input.git"
  },
  "devDependencies": {
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}